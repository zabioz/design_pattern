package singleton;

public class Car {

	public static int serialNum = 10001;
	public int CarNum;
	public static Car CreateCar;
	
	public Car() {
		serialNum++;
		CarNum = serialNum;

	}

	public int getCarNum() {
		// TODO Auto-generated method stub
		return CarNum;
	}

	public static Car CreateCar() {
		// TODO Auto-generated method stub
		return CreateCar;
	}
	
}